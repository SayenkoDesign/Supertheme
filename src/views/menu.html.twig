{% if menu %}
    <ul>
        {% for item in menu %}
            <li class="{{item.classes | join(' ')}}">
                <a href="{{item.get_link}}">{{item.title}}</a>
                {% include "menu.html.twig" with {'menu': item.get_children} %}
            </li>
        {% endfor %}
    </ul>
{% endif %}
